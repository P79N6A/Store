    -khtml-border-radius: 0px;
    /* KHTML border radius */
    -icab-border-radius: 0px;
    /* icab border radius */
    border-radius: 0px;
    /* CSS3 Opera border radius */
    -opera-border-radius: 0px;
    -o-border-radius: 0px;
    -border-radius: 0px;
}

