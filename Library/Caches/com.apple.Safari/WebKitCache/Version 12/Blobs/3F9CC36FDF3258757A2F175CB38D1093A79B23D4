  <script>

    new mlPushMenu( document.getElementById( 'mp-menu' ), document.getElementById( 'trigger' ) );

    $(function() {
      $('.lazy-load').lazy({ visibleOnly: true })
      $('#scrollable-container .lazy-load').lazy({
        visibleOnly: true,
        appendScroll: $('#scrollable-container')
      })
    })

    $('#mobile-menu-open').on('click', function() {
      $('#mobile-menu').addClass('open')
      $(document.body).addClass('modal-open')
    })

    $('#mobile-menu-close').on('click', function() {
      $('#mobile-menu').removeClass('open')
      $(document.body).removeClass('modal-open')
    })

    $('.scroller').scroll(function() {
      if ( $('.scroller').scrollTop() >= 10 ) {
        $('.main-navigation').addClass('after-scroll');
      } else {
        $('.main-navigation').removeClass('after-scroll');
      }
    });

  </script>

    <script>

    var dropDownBtn = $('.drop-select');
    var dropDown = $('.dropdown');

    dropDownBtn.click(function(){
      dropDownBtn.removeClass('active');
      $(this).addClass('active');
    })

    $(document).click(function(e) {

      const isClose = $(e.target)[0].className === 'close';
      const isNotMenu = $(e.target).closest('nav').length === 0;

      if ( isClose || isNotMenu ) {
        dropDownBtn.removeClass('active');
      }
    });

  </script>


