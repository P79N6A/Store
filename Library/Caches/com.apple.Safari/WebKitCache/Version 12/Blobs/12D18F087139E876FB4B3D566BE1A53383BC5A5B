	<script>
        $(document).ready(function(){
            if(i.user){
                if(i.user.AcceptedEmail){
                    $('#receive-emails').remove();
                }
            }
            if(i.authenticated){

		        $('#referralUrl').val(i.user.ReferralUrl);


		        $('.linkedin-share').attr('href','https://www.linkedin.com/shareArticle?mini=true&url='+i.user.ReferralUrl+'&title=Give%20yourself%20a%20professional%20look%20in%20no%20time.%20Free%20CV%20Builder&summary=&source=');
		        $('.facebook-share').attr('href','https://www.facebook.com/sharer/sharer.php?u='+i.user.ReferralUrl);
		        $('.twitter-share').attr('href','https://twitter.com/home?status=Give%20yourself%20a%20professional%20look%20in%20no%20time.%20Free%20CV%20Builder%20'+i.user.ReferralUrl);
		        $('.reddit-share').attr('href','http://reddit.com/submit?url='+i.user.ReferralUrl+'&title=Give%20yourself%20a%20professional%20look%20in%20no%20time.%20Free%20CV%20Builder');
		        //console.log(i.referredUsers);
		        if(i.referredUsers >= 1 || i.boughtProducts.includes('package_premium-templates')){
		            $('#referral-lightbox').css('display','none');
		            $('#cover-letters').css('display','block');
		        }
            }else if(window.location.href.indexOf("letter") > -1) {
				window.location.replace("https://www.cv-template.com/en/login");
			}
        });
    </script>
