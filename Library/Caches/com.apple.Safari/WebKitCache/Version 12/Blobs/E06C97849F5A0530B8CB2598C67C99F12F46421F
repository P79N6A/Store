{var now=new Date();var hour=now.getHours();var minute=now.getMinutes();var second=now.getSeconds();var ap="A.M.";if(hour>11){ap="P.M.";}
if(hour>12){hour=hour-12;}
if(hour==0){hour=12;}
if(hour<10){hour=""+hour;}
if(minute<10){minute="0"+minute;}
if(second<10){second="0"+second;}
var timeString=hour+':'+
minute+' '+
ap;return timeString;}
