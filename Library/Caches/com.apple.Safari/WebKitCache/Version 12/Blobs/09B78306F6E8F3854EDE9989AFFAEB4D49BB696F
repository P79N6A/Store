<script>
  window.fixWidgetWindowLocation = function () {
    var distanceFromTop = $(window).scrollTop()
    var windowHeight = $(window).height()
    var windowBottomFromPageTop = distanceFromTop + windowHeight

    var widgetFromTop = $('#widget-button').offset().top
    var widgetHeight = $('#widget-button').outerHeight(true)

    var widgetButtonFromPageTop = widgetFromTop + widgetHeight


    if (windowBottomFromPageTop -  widgetButtonFromPageTop > 0) {
      var bodyHeight = document.body.offsetHeight
      var widgetDistanceFromBottom = bodyHeight - widgetButtonFromPageTop

      $('#webWidget').css('bottom', widgetDistanceFromBottom)
    } else {
      $('#webWidget').css('bottom', 0)
    }
  }
</script>
